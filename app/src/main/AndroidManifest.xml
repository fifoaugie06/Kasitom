<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kasitom">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_kasitom"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_kasitom"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".antonimSinonim.SinonimActivity" />
        <activity android:name=".antonimSinonim.AntonimActivity" />
        <activity android:name=".quiz.ScoreboardActivity" />
        <activity android:name=".quiz.QuizActivity" />
        <activity android:name=".chatroom.GlobalChat" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>